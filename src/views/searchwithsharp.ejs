<!DOCTYPE html>
<html lang="en">
<meta http-equiv="content-type" content="text/html;charset=utf-8">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Photography - Portfolio</title>
    <!-- Bootstrap core CSS -->

    <link href="/css/jasny-bootstrap.min.css" rel="stylesheet">
    <link href='http://fonts.googleapis.com/css?family=Raleway' rel='stylesheet' type='text/css'>
    <link href="/css/bootstrap.min.css" rel="stylesheet">
    <!-- Custom styles for this template -->
    <link href="/css/navmenu-reveal.css" rel="stylesheet">
    <link href="/css/style.css" rel="stylesheet">
    <link href="/css/full-slider.css" rel="stylesheet">
    <link href="/css/Icomoon/style.css" rel="stylesheet" type="text/css" />
    <link href="/css/animated-masonry-gallery.css" rel="stylesheet" type="text/css" />
    <link href="/css/lightbox.css" rel="stylesheet" type="text/css" />

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>

<body>
    <div class="bar">
        <button type="button" class="navbar-toggle" data-toggle="offcanvas" data-recalc="false" data-target=".navmenu" data-canvas=".canvas">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
        </button>
    </div>
    <div class="navmenu navmenu-default navmenu-fixed-left in">
        <ul class="nav navmenu-nav">
            <li><a href="/app/website">Home</a></li>
            <li><a></a></li>
        </ul>
        <form role="form" action="/search/postyear" method="post">
            <div class="form-group">
              <label for="year"><span class="glyphicon glyphicon-user"></span>Year:</label>
              <input type="text" class="form-control" name="year" placeholder="Enter year">
            </div>
        </form>
        <form role="form" action="/search/posttext" method="post">
            <div class="form-group">
              <label for="year"><span class="glyphicon glyphicon-user"></span>Search all fields for Text:</label>
              <input type="text" class="form-control" name="text" placeholder="Enter text">
            </div>
        </form>

    </div>

    <div class="canvas canvas-slid"><br>
        <div id="container" class="container">

            <div class="row marginr20">

                <h1 class="blog-post-title text-center"><%=title%></h1>
                <span class="title-divider"></span>

                <div class="col-md-6 col-sm-6 col-xs-4">
                    <div id="gallery-header-center-left-title hidden"></div>
                    <div id="search-header-center-left-title">Page <%=page+1%> of <%=total%></div>
                </div>

                <div class="col-md-6 col-sm-6 col-xs-8">
                    <% if (!photoend) {%>
                        <a href="/search/skip/<%=stype%>/+" class="gallery-header-center-right-links" id="nextpage">Next...</a>
                    <%}%>
                    <% for(var i = Math.floor((results.length-1)/10)+1; i > 0; i--) {%>
                        <div class="gallery-header-center-right-links" alt="<%=results[(i-1)*10].subfolder%>" id="filter-<%=i%>"><%=i+Math.floor(indexnav/10)%>
                        </div>
                    <%}%>
                    <% if (indexnav > 0) {%>
                        <a href="/search/skip/<%=stype%>/-" class="gallery-header-center-right-links" id="prevpage">...Prev</a>
                    <%}%>
                </div>
            </div>

            <% if (isMovie) {%>
            <div class="row marginr20">
                <div class="col-md-12 col-sm-12 col-xs-12 text-center">
                    <a class="btn btn-primary" href="/search/video<%=vtype%>" >Videos</a>
                </div>
            </div>
            <%}%>
            <br>
            <div class="row">
            <br>
            <div id="gallery">
                <div id="gallery-content">
                    <div id="gallery-content-center">
                        <% for(var i = 0; i < results.length; i++) {%>
                            <% if (results[i].filename.indexOf('specialdiapos') !== -1) {%>
                                <a href="/navigation/diapos/<%=results[i].filename.substr(results[i].filename.indexOf('specialdiapos') + 13,3)%>">
                            <%} else {%>
                                <a href="/assets/<%=results[i].theme%>/<%=results[i].folder%>/<%=results[i].filename%>" data-lightbox="studio1">
                            <%}%>
                            <img src="/sharp/temp<%=i%>"
                            class="<%=Math.floor((i/10) + 1)%>"
                            alt="Dim: <%=results[i].photoDimW%> x <%=results[i].photoDimH%>, Photo: <%=results[i].theme%>/<%=results[i].folder%>/<%=results[i].filename%> <%=results[i].camera%>
                            <%=results[i].description%> <%=results[i].reverseGeo%>" id="photo<%=i%>">  </a>
                        <%}%>
                    </div>
                </div>
            </div>
            </div>
        </div>
    </div>

    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script type="text/javascript" src="/js/jquery.js"></script>
    <script type="text/javascript" src="/js/jquery-ui-1.10.4.min.js"></script>
    <script src="/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="/js/isotope.pkgd.min.js"></script>
    <script type="text/javascript" src="/js/animated-masonry-gallery.js"></script>
    <script src="/js/jasny-bootstrap.min.js"></script>
    <script src="/js/lightbox.ejs"></script>

</body>
</html>